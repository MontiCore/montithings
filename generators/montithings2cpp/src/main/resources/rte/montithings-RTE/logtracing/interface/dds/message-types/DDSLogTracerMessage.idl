module DDSLogTracerMessage {
  enum RequestData { LOG_ENTRIES,
                     INTERNAL_DATA };

    @topic
    struct Request {
        @key string target_instance;

        string req_uuid;
        string log_uuid;
        string input_uuid;
        string output_uuid;

        long from_timestamp;
        RequestData req_data;
    };

    @topic
    struct Response {
        string req_uuid;
        RequestData req_data;

        string content;
    };
};
